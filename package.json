{
  "name": "url-shortener",
  "version": "1.0.0",
  "description": "A Nodejs app for URL SHORTENER",
  "main": "app.js",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "dev:server": "nodemon app.js",
    "start:client": "HOST=0.0.0.0 PORT=3000 npm run dev --prefix client",
    "dev": "concurrently \"npm run dev:server\" \"npm run start:client\""
  },
  "keywords": [
    "nodejs",
    "javascript",
    "mongodb",
    "express'"
  ],
  "author": {
    "name": "Bineet Naidu",
    "url": "https://bineetnaidu.web.app"
  },
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.6"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.10.15",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.17"
  }
}
